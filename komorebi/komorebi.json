{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/komorebi.app.json",
  "bar_configurations": ["$Env:USERPROFILE/komorebi.bar.json"],
  "border": true,
  "border_offset": -1,
  "border_width": 10,
  "default_workspace_padding": 5,
  "default_container_padding": 5,
  "floating_window_aspect_ratio": "Widescreen",
  "window_hiding_behaviour": "Cloak",
  "stackbar": {
    "height": 50,
    "label": "Process",
    "mode": "OnStack",
    "tabs": {
      "width": 200
    }
  },
  "theme": {
    "palette": "Catppuccin",
    "name": "Macchiato"
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP",
          "layout_flip": "Horizontal"
        },
        {
          "name": "II",
          "layout": "Grid"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "null"
        }
      ]
    }
  ],
  "floating_applications": [
    {
      "kind": "Exe",
      "id": "neovide.exe",
      "matching_strategy": "Equals"
    }
  ],
  "ignore_rules": [
    {
      "kind": "Exe",
      "id": ".*",
      "matching_strategy": "Regex"
    }
  ],
  "manage_rules": [
    {
      "kind": "Exe",
      "id": "neovide.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "SumatraPDF.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "wezterm-gui.exe",
      "matching_strategy": "Equals"
    }
  ]
}
